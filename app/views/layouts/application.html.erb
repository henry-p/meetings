<!DOCTYPE html>
<html>
<head>
  <title>Meetings</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if logged_in? %>
    <div id="top-banner">
  </div>
  <header>
    <div class='container'>
      <span id='nav-bar-icons'>
      </span>
      <span id='nav-bar-greeting'>
        <%= image_tag current_user.image_path, id: 'header-google-img' %>
        <%= link_to "Logout", logout_path, method: :delete %> |
        <%= link_to "Profile", profile_path %>
      </span>
    </div>
  </header>
<% end %>

<%= yield %>

</body>
</html>
