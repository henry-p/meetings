<!DOCTYPE html>
<html>
<head>
  <title>Meetings</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if logged_in? %>
  <div id="top-banner">
  </div>
  <header>
    <div class='container'>
      <span id='nav-bar'>
        <% if current_user.image_path %>
          <%= link_to (image_tag current_user.image_path, id: 'header-google-img'), profile_path %>
        <% end %>
        <%= link_to "Home", profile_path %>
        <%= link_to ("Archive"), profile_archive_path %>
        <%= link_to "Logout", logout_path, method: :delete %>
    </div>
  </header>
<% end %>

<%= yield %>

</body>
</html>